{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "Borrow Attempts (rate)",
      "gridPos": { "h": 6, "w": 6, "x": 0, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum(rate(license_borrow_attempts_total[5m]))",
          "legendFormat": "attempts/s",
          "refId": "A"
        }
      ]
    },
    {
      "type": "stat",
      "title": "Borrow Success (rate)",
      "gridPos": { "h": 6, "w": 6, "x": 6, "y": 0 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum(rate(license_borrow_success_total[5m]))",
          "legendFormat": "success/s",
          "refId": "A"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Current Borrowed per Tool",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 0 },
      "options": { "displayMode": "gradient", "orientation": "horizontal" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "licenses_borrowed",
          "legendFormat": "{{tool}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Borrow Failures by Reason (rate)",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "sum by (reason) (rate(license_borrow_failure_total[5m]))",
          "legendFormat": "{{reason}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Borrow Duration (p95)",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "histogram_quantile(0.95, sum by (le) (rate(license_borrow_duration_seconds_bucket[5m])))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    },
    {
      "type": "table",
      "title": "Pool: Total / Borrowed / Available",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 14 },
      "options": { "showHeader": true },
      "transformations": [
        { "id": "labelsToFields" },
        { "id": "organize", "options": { "excludeByName": {"quantile": true} } }
      ],
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "licenses_borrowed",
          "legendFormat": "borrowed|{{tool}}",
          "refId": "A"
        },
        {
          "datasource": { "type": "prometheus", "uid": "Prometheus" },
          "expr": "licenses_borrowed * 0",
          "legendFormat": "total|{{tool}}",
          "refId": "B"
        }
      ]
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["license", "demo"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timezone": "",
  "title": "License Server Overview",
  "version": 1
}


